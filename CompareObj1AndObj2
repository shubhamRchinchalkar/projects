var obj1 = {
  "name": "person1",
  "age": 5
};

var obj2 = {
  "age": 5,
  "name": "person1"
};
s
function areObjectsEqual(obj1, obj2) {
  const jsonString1 = JSON.stringify(obj1, Object.keys(obj1).sort());
  const jsonString2 = JSON.stringify(obj2, Object.keys(obj2).sort());
  return jsonString1 === jsonString2;
}

console.log(areObjectsEqual(obj1, obj2));